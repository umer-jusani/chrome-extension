{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Chrome Extension",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting", "nativeMessaging"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "32": "icon32.png"
  }
}
